    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button onAction="#onHelloButtonClick" text="Hello!" />

    <Label fx:id="welcomeText" />
   <Label text="Label" />
   <Button fx:id="btTest" mnemonicParsing="false" onAction="#onBtTestAction" text="Button" />
   <ImageView fitHeight="200.0" fitWidth="200.0">
      <image>
         <Image url="@../esdeath.jpg" />
      </image>
   </ImageView>
</VBox>
